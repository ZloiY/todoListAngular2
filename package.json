{
  "name": "todo-list-angular2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": " npm run build:sass && ng serve -o",
    "build": "npm run build:sass && ng build",
    "prod-build": "npm run build:sass && ng build --prod --aot",
    "prod-deploy": "npm run build:sass && ng serve --prod --aot",
    "test": "npm run build:sass && ng test",
    "lint": "ng lint",
    "e2e": "npm run build:sass && ng e2e",
    "build:sass": "node-sass src/styles.scss -o src/ && node-sass src/app/todolist/todo-list.component.scss -o src/app/todolist/ && npm run build_todo-list-checkall-del:sass && npm run build_todo-list-input:sass && npm run build_todo-list-sort:sass && npm run build_todo-list-task-element:sass",
    "build_todo-list-checkall-del:sass": "node-sass src/app/todolist/todo-list-checkall-del/todo-list-checkall-del.component.scss -o src/app/todolist/todo-list-checkall-del/",
    "build_todo-list-input:sass": "node-sass src/app/todolist/todo-list-input/todo-list-input.component.scss -o src/app/todolist/todo-list-input/",
    "build_todo-list-sort:sass": "node-sass src/app/todolist/todo-list-sort/todo-list-sort.component.scss -o src/app/todolist/todo-list-sort",
    "build_todo-list-task-element:sass": "node-sass src/app/todolist/todo-list-task-element/todo-list-task-element.component.scss -o src/app/todolist/todo-list-task-element",
    "watch:sass": "npm run build:sass && node-sass src/app/todolist/scss/ -wo src/app/todolist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "node-sass": "^4.5.3",
    "rxjs": "^5.1.0",
    "sass-loader": "^6.0.6",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.1.2",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "2.5.45",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
